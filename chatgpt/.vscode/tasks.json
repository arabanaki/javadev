{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "mvn: clean package",
      "type": "shell",
      "command": "mvn -q clean package",
      "group": "build",
      "options": {
        "cwd": "${workspaceFolder}/chatgpt"
      },
      "problemMatcher": ["$javac"]
    },
    {
      "label": "mvn: run",
      "type": "shell",
      "command": "mvn -q exec:java",
      "options": {
        "cwd": "${workspaceFolder}/chatgpt"
      }
    }
  ]
}
